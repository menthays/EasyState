    "@pika/pack": "0.3.7",
    "@pika/plugin-build-node": "0.6.0",
    "@pika/plugin-build-types": "0.6.0",
    "@pika/plugin-build-web": "0.6.0",
    "@pika/plugin-standard-pkg": "0.6.0",
    "@types/jest": "24.0.17",
    "@types/react": "16.9.2",
    "jest": "24.9.0",
    "react": "16.9.0",
