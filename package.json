{
  "name": "@menthays/easystate",
  "version": "0.2.1",
  "license": "MIT",
  "description": "Easy state management based on React Context and Hooks",
  "repository": {
    "type": "git",
    "url": "https://github.com/menthays/EasyState.git"
  },
  "scripts": {
    "test": "jest",
    "tsc": "tsc --noEmit",
    "build": "pack build",
    "publish": "pack publish"
  },
  "babel": {
    "presets": [
      "@babel/preset-typescript",
      "@babel/preset-react"
    ],
    "env": {
      "test": {
        "presets": [
          "@babel/preset-env"
        ]
      }
    }
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-standard-pkg"
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-build-web"
      ],
      [
        "@pika/plugin-build-types"
      ]
    ]
  },
  "peerDependencies": {
    "react": ">=16.8.1"
  },
  "devDependencies": {
    "@babel/core": "7.4.0",
    "@babel/preset-env": "7.4.2",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.3.3",
    "@pika/pack": "0.3.6",
    "@pika/plugin-build-node": "0.3.14",
    "@pika/plugin-build-types": "0.3.14",
    "@pika/plugin-build-web": "0.3.14",
    "@pika/plugin-standard-pkg": "0.3.14",
    "@types/jest": "24.0.11",
    "@types/react": "16.8.8",
    "jest": "24.5.0",
    "react": "16.8.5",
    "react-dom": "16.8.5",
    "react-hooks-testing-library": "0.3.8",
    "react-testing-library": "6.0.3",
    "typescript": "3.3.4000"
  }
}
